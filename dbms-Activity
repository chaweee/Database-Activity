-- Create the database
CREATE DATABASE sari_sari_store;

-- Use the database
USE sari_sari_store;

-- Create the table
CREATE TABLE inventory (
    CuID INT PRIMARY KEY,
    Food VARCHAR(50),
    Beverage VARCHAR(50),
    Condiment VARCHAR(50),
    Utility VARCHAR(50)
);

